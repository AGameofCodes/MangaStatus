{
  "name": "mangastatus-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "node": ">=21.0.0",
  "author": "wea_ondara",
  "license": "ISC",
  "scripts": {
    "dev": "vite",
    "clean": "find src | grep -E '((\\.d\\.ts)|(\\.js(\\.map)?))$' | xargs rm",
    "build": "run-s type-check build-only test:unit",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/ --passWithNoTests --run",
    "test:e2e": "start-server-and-test preview :4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' :4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@intlify/unplugin-vue-i18n": "^6.0.3",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3",
    "bootstrap-vue-next": "^0.26.18",
    "html-entities": "^2.5.2",
    "idb": "^8.0.1",
    "pinia": "^2.3.0",
    "pinia-class-component": "^0.9.4",
    "string-similarity-js": "^2.1.4",
    "vue": "^3.4.21",
    "vue-facing-decorator": "^3.0.4",
    "vue-good-table-next": "^0.2.2",
    "vue-i18n": "^11.0.1",
    "vue-router": "^4.5.0",
    "vue3-toastify": "^0.2.8"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/bootstrap": "^5.2.10",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.2.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cypress": "^13.17.0",
    "eslint": "^9.17.0",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-vue": "^9.32.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.4.2",
    "sass": "^1.83.0",
    "start-server-and-test": "^2.0.9",
    "typescript": "~5.7.2",
    "vite": "^6.0.6",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.2.0"
  }
}
